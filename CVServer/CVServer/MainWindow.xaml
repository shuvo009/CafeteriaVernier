										<Binding ElementName="EmployeeGridView" Path="SelectedItem.PhoneNmber" Mode="TwoWay" UpdateSourceTrigger="LostFocus" ValidatesOnDataErrors="True">
											<Binding.ValidationRules>
												<local:PhoneNumber/>
											</Binding.ValidationRules>
										</Binding>
								</dxe:TextEdit.Text>
							 </dxe:TextEdit>
