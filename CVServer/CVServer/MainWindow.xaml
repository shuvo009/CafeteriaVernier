    		<Grid.RowDefinitions>
    			<RowDefinition Height="0.903*"/>
    			<RowDefinition Height="0.097*"/>
    		</Grid.RowDefinitions>
    		<Grid.ColumnDefinitions>
    			<ColumnDefinition Width="0.735*"/>
    			<ColumnDefinition Width="0.265*"/>
    		</Grid.ColumnDefinitions>
            <telerik:RadGridView x:Name="EmployeeGridView" Margin="0,0,0,0.335" ShowGroupPanel="False" Grid.Column="1" FontSize="14.667" AutoGenerateColumns="False">
            	<telerik:RadGridView.Columns>
            		<telerik:GridViewDataColumn Header="Employee ID`s" HeaderTextAlignment="Center" DataMemberBinding="{Binding Name}" MinWidth="130"/>        	
				</telerik:RadGridView.Columns>         
			</telerik:RadGridView>
            <StackPanel Margin="71.99,0,54.856,8.001" Orientation="Horizontal" Grid.Row="1" VerticalAlignment="Bottom" Height="31.69" HorizontalAlignment="Center">
            	<Procesta_CvServer_UserControls:ButtonWithImages x:Name="EmployeeNew" Content="New" Margin="0" FontSize="14.667" Width="70.834" Command="{Binding NewEmployeeCommand, ElementName=window, Mode=OneWay}" CommandParameter="Parameter No Need" TabIndex="7" ButtonImages="ButtonImages/Add.png"/>
            	<Procesta_CvServer_UserControls:ButtonWithImages x:Name="EmployeeUpdate" Content="Update" Margin="5,0,0,0" Width="89.333" FontSize="14.667" Command="{Binding UpdateEmployeeCommand, ElementName=window, Mode=OneWay}" CommandParameter="{Binding SelectedItem, ElementName=EmployeeGridView}" TabIndex="8" ButtonImages="ButtonImages/Save.png"/>
            	<Procesta_CvServer_UserControls:ButtonWithImages x:Name="EmployeeDelete" Content="Detete" Margin="5,0,0,0" Width="86.333" FontSize="14.667" Command="{Binding DeleteEmployeeCommand, ElementName=window, Mode=OneWay}" CommandParameter="{Binding SelectedItem, ElementName=EmployeeGridView}" TabIndex="8" ButtonImages="ButtonImages/Delete.png" IsEnabled="{Binding LoginEmployee, ConverterParameter=EmployeeEdit, Converter={StaticResource Editpermission}, ElementName=window}"/>
            	<Procesta_CvServer_UserControls:ButtonWithImages x:Name="EmployeeReport" Content="Report" Margin="5,0,0,0" Width="80.333" FontSize="14.667" Command="{Binding ReportEmployeeCommand, ElementName=window, Mode=OneWay}" CommandParameter="{Binding SelectedItem, ElementName=EmployeeGridView}" TabIndex="10" ButtonImages="ButtonImages/Report.png"/>
            </StackPanel>
            <dx:DXTabControl Margin="0,0,0.083,0.088">
            	<dx:DXTabItem FontSize="14.667" MinWidth="100" HorizontalContentAlignment="Center" Header="Employee">
            		<Grid>
                			<Grid.RowDefinitions>
                				<RowDefinition Height="0.051*"/>
                				<RowDefinition Height="0.081*"/>
                				<RowDefinition Height="0.105*"/>
                				<RowDefinition Height="0.105*"/>
                				<RowDefinition Height="0.063*"/>
                				<RowDefinition Height="0.105*"/>
                				<RowDefinition Height="0.419*"/>
                				<RowDefinition Height="0.069*"/>
                			</Grid.RowDefinitions>
                			<Grid.ColumnDefinitions>
                				<ColumnDefinition Width="0.331*"/>
                				<ColumnDefinition Width="0.353*"/>
                				<ColumnDefinition Width="0.316*"/>
                			</Grid.ColumnDefinitions>
