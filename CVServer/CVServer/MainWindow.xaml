    					<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
    						<GradientStop Color="DarkGray" Offset="0"/>
    						<GradientStop Color="#FF999999" Offset="1"/>
    						<GradientStop Color="Gray" Offset="0.504"/>
    					</LinearGradientBrush>
    				</Border.BorderBrush>
    				<Image Source="{Binding Image, Converter={StaticResource ByteImageConverter}}"/>
    			</Border>
    			<TextBlock Margin="6,13.299,0,8" TextWrapping="Wrap" Text="{Binding UserName}" FontSize="16"/>
    		</StackPanel>
    	</DataTemplate>
		<!--Data Member for List view-->
		<DataTemplate x:Key="ListBoxDataTamplate">
			<DockPanel Height="40" Margin="0,0,0,5" LastChildFill="True">
				<Border Margin="0,5,0,0" Height="36" Width="34" BorderThickness="2" CornerRadius="2" DockPanel.Dock="Left">
					<Border.BorderBrush>
						<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
							<GradientStop Color="#FFE5E5E5" Offset="0"/>
							<GradientStop Color="#FFCACACA" Offset="1"/>
							<GradientStop Color="#FFE5E5E5" Offset="0.502"/>
						</LinearGradientBrush>
					</Border.BorderBrush>
					<Image Source="{Binding Image}"/>
				</Border>
				<TextBlock DockPanel.Dock="Left" Height="19" TextWrapping="Wrap" Text="{Binding Name}" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14.667"/> 
			</DockPanel>
		</DataTemplate>
    	
		
		<Style x:Key="TileViewItemHeaderStyle1" TargetType="{x:Type tileview:TileViewItemHeader}">
