				    	<Grid.ColumnDefinitions>
				    		<ColumnDefinition Width="0.371*"/>
				    		<ColumnDefinition Width="0.629*"/>
				    	</Grid.ColumnDefinitions>
				    	<Grid.RowDefinitions>
				    		<RowDefinition Height="0.517*"/>
				    		<RowDefinition Height="0.483*"/>
				    	</Grid.RowDefinitions>
				    	<Border BorderThickness="5" Margin="8,8,24.442,14.328" CornerRadius="5" Width="100" Height="100">
				    		<Border.BorderBrush>
				    			<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
				    				<GradientStop Color="#FFCECECE" Offset="0"/>
				    				<GradientStop Color="#FFE4E4E4" Offset="1"/>
				    				<GradientStop Color="#FF626262" Offset="0.504"/>
				    			</LinearGradientBrush>
				    		</Border.BorderBrush>
				    		<Image Source="{Binding Status, Converter={StaticResource CounterStatusToImagePath}}"/>
				    	</Border>
				    	<Procesta_CvServer_UserControls:MenuButton Margin="-1.5,42.471,0,39.857" Foreground="#FFE0E0E0" ButtonImage="Images/restart.png" ScreenTip="Restart" SupperTip="Restart Client Computer" Grid.Column="1" HorizontalAlignment="Left" Width="100" Command="{Binding DataContext.ClientRestartCommand, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Window}}}" CommandParameter="{Binding CounterNumber}"/>
				    	<Procesta_CvServer_UserControls:MenuButton Margin="121.5,42.471,130.069,39.857" Foreground="#FFE0E0E0" ButtonImage="Images/shutdown.png" ScreenTip="Shutdown" SupperTip="Shutdown Client Computer" Grid.Column="1" Command="{Binding DataContext.ClientShoutdownCommand, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Window}}}" CommandParameter="{Binding CounterNumber}" Width="100" HorizontalAlignment="Center"/>
				    	<Procesta_CvServer_UserControls:MenuButton Margin="0,42.471,8,39.857" Foreground="#FFE0E0E0" HorizontalAlignment="Right" Width="100" ButtonImage="Images/LogOff.png" ScreenTip="Logoff" SupperTip="Logoff customer account if he is currently logged in" Grid.Column="1" Command="{Binding DataContext.ClientLogoffCommand, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Window}}}" CommandParameter="{Binding CounterNumber}"/>
				    	<TextBlock Margin="0,7.999,8,0" TextWrapping="Wrap" Text="Counter Name :" VerticalAlignment="Top" FontSize="16" Foreground="#FFE0E0E0" Grid.Row="1" HorizontalAlignment="Right" Width="112"/>
				    	<TextBlock Margin="0,33.279,8,0" TextWrapping="Wrap" Text="Counter Number :" FontSize="16" Foreground="#FFE0E0E0" Grid.Row="1" VerticalAlignment="Top" HorizontalAlignment="Right" Width="128"/>
				    	<TextBlock Margin="0,58.556,8,0" TextWrapping="Wrap" Text="Customer Name :" VerticalAlignment="Top" FontSize="16" Foreground="#FFE0E0E0" Grid.Row="1" HorizontalAlignment="Right" Width="123"/>
				    	<TextBlock Margin="0,7.994,0,0" TextWrapping="Wrap" Text="{Binding CounterName}" VerticalAlignment="Top" FontSize="16" Foreground="#FFE0E0E0" Grid.Row="1" Grid.Column="1" HorizontalAlignment="Left"/>
				    	<TextBlock Margin="0,33.275,0,0" TextWrapping="Wrap" Text="{Binding CounterNumber}" FontSize="16" Foreground="#FFE0E0E0" Grid.Row="1" VerticalAlignment="Top" Grid.Column="1" HorizontalAlignment="Left" Width="127.404"/>
				    	<TextBlock Margin="0,58.556,0,0" TextWrapping="Wrap" Text="{Binding CustomerID}" VerticalAlignment="Top" FontSize="16" Foreground="#FFE0E0E0" Grid.Row="1" Grid.Column="1" HorizontalAlignment="Left"/>
				    </Grid>  
           </telerikTheme:RadFluidContentControl.LargeContent>
       </telerikTheme:RadFluidContentControl>
        </DataTemplate>
		
		<DataTemplate x:Key="headerTemplate">
			<StackPanel Orientation="Horizontal">
				<TextBlock Text="{Binding CounterName}" Foreground="Black"/>
			</StackPanel>
        </DataTemplate>


		

    	<Storyboard x:Key="EmployeeLoginAnimation">
    		<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.X)" Storyboard.TargetName="EmployeeLoginUsername">
    			<EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="29.307"/>
    		</DoubleAnimationUsingKeyFrames>
    		<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.X)" Storyboard.TargetName="textBlock1">
    			<EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="29.307"/>
    		</DoubleAnimationUsingKeyFrames>
    		<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)" Storyboard.TargetName="EmployeeLoginUsername">
    			<EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="0.21"/>
    		</DoubleAnimationUsingKeyFrames>
    		<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)" Storyboard.TargetName="textBlock1">
    			<EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="0.21"/>
    		</DoubleAnimationUsingKeyFrames>
    		<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="EmployeeLoginUsername">
    			<EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="1"/>
    		</DoubleAnimationUsingKeyFrames>
    		<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="textBlock1">
    			<EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="1"/>
    		</DoubleAnimationUsingKeyFrames>
    		<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.X)" Storyboard.TargetName="passwordBoxEdit">
    			<EasingDoubleKeyFrame KeyTime="0:0:0.8" Value="28.967"/>
    		</DoubleAnimationUsingKeyFrames>
    		<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.X)" Storyboard.TargetName="textBlock2">
    			<EasingDoubleKeyFrame KeyTime="0:0:0.8" Value="28.967"/>
    		</DoubleAnimationUsingKeyFrames>
    		<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="passwordBoxEdit">
    			<EasingDoubleKeyFrame KeyTime="0:0:0.8" Value="1"/>
    		</DoubleAnimationUsingKeyFrames>
    		<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="textBlock2">
    			<EasingDoubleKeyFrame KeyTime="0:0:0.8" Value="1"/>
    		</DoubleAnimationUsingKeyFrames>
    		<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.X)" Storyboard.TargetName="EmployeeLogin">
    			<EasingDoubleKeyFrame KeyTime="0:0:1" Value="26.5"/>
    		</DoubleAnimationUsingKeyFrames>
    		<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="EmployeeLogin">
    			<EasingDoubleKeyFrame KeyTime="0:0:1" Value="1"/>
    		</DoubleAnimationUsingKeyFrames>
    	</Storyboard>
	</Window.Resources>
	<Window.Triggers>
		<EventTrigger RoutedEvent="FrameworkElement.Loaded" SourceName="PanelEmployeeLogin">
		  <BeginStoryboard Storyboard="{StaticResource EmployeeLoginAnimation}"/>
		</EventTrigger>
	</Window.Triggers>
	<Window.InputBindings>
		<KeyBinding Command="{Binding MainMenuCommand, ElementName=window, Mode=OneWay}" Key="C" Modifiers="Control" CommandParameter="NewCustomer"/>
		<KeyBinding Command="{Binding MainMenuCommand, ElementName=window, Mode=OneWay}" Key="R" Modifiers="Control" CommandParameter="AccountRecharge"/>
		<KeyBinding Command="{Binding MainMenuCommand, ElementName=window, Mode=OneWay}" Key="E" Modifiers="Control" CommandParameter="RechargeHistory"/>
		<KeyBinding Command="{Binding MainMenuCommand, ElementName=window, Mode=OneWay}" Key="L" Modifiers="Control" CommandParameter="CustomerLoginHistory"/>
		<KeyBinding Command="{Binding MainMenuCommand, ElementName=window, Mode=OneWay}" Key="A" Modifiers="Control" CommandParameter="Cash"/>
		<KeyBinding Command="{Binding MainMenuCommand, ElementName=window, Mode=OneWay}" Key="S" Modifiers="Control" CommandParameter="Summary"/>
		<KeyBinding Command="{Binding SettingMenuCommand, ElementName=window, Mode=OneWay}" Key="U" Modifiers="Control" CommandParameter="RateSetting"/>	<!--Setting-->	
		<KeyBinding Command="{Binding MainMenuCommand, ElementName=window, Mode=OneWay}" Key="I" Modifiers="Control" CommandParameter="Setting"/>
		<KeyBinding Command="{Binding MainMenuCommand, ElementName=window, Mode=OneWay}" Key="N" Modifiers="Control" CommandParameter="TeamMaintenance"/>
		<KeyBinding Command="{Binding MainMenuCommand, ElementName=window, Mode=OneWay}" Key="O" Modifiers="Control" CommandParameter="CustomerMaintenance"/>
		<KeyBinding Command="{Binding MainMenuCommand, ElementName=window, Mode=OneWay}" Key="M" Modifiers="Control" CommandParameter="NewTeam"/>
		<KeyBinding Command="{Binding SettingMenuCommand, ElementName=window, Mode=OneWay}" Key="P" Modifiers="Control" CommandParameter="ChangePassword"/> <!--Setting-->
		<KeyBinding Command="{Binding SettingMenuCommand, ElementName=window, Mode=OneWay}" Key="F" Modifiers="Control" CommandParameter="ScreenCapture"/> <!--Setting-->
		<KeyBinding Command="{Binding MainMenuCommand, ElementName=window, Mode=OneWay}" Key="J" Modifiers="Control" CommandParameter="SendEmail"/>
		<KeyBinding Command="{Binding SettingMenuCommand, ElementName=window, Mode=OneWay}" Key="K" Modifiers="Control" CommandParameter="EMailSetting"/> <!--Setting-->
		<KeyBinding Command="{Binding SettingMenuCommand, ElementName=window, Mode=OneWay}" Key="G" Modifiers="Control" CommandParameter="GeneralSetting"/><!--Setting-->
		<KeyBinding Command="{Binding MainMenuCommand, ElementName=window, Mode=OneWay}" Key="H" Modifiers="Control" CommandParameter="Home"/>
		<KeyBinding Command="{Binding SettingMenuCommand, ElementName=window, Mode=OneWay}" Key="T" Modifiers="Control" CommandParameter="CounterSetting"/><!--setting-->
		<KeyBinding Command="{Binding MainMenuCommand, ElementName=window, Mode=OneWay}" Key="Q" Modifiers="Control" CommandParameter="NewEmployee"/>		
		<KeyBinding Command="{Binding MainMenuCommand, ElementName=window, Mode=OneWay}" Key="D" Modifiers="Control" CommandParameter="Database"/>
		<KeyBinding Command="{Binding MainMenuCommand, ElementName=window, Mode=OneWay}" Key="Z" Modifiers="Control" CommandParameter="CustomerStatusReset"/>
		<KeyBinding Command="{Binding MainMenuCommand, ElementName=window, Mode=OneWay}" Key="Y" Modifiers="Control" CommandParameter="CashHistory"/>
	</Window.InputBindings>
	<Grid x:Name="PanelRoot" DataContext="{Binding Source={StaticResource DateTimeDataSource}}">
    	<Grid.ColumnDefinitions>
    		<ColumnDefinition Width="0.115*"/>
    		<ColumnDefinition Width="0.764*"/>
    		<ColumnDefinition Width="0.121*"/>
    	</Grid.ColumnDefinitions>
    	<Grid.RowDefinitions>
    		<RowDefinition Height="0.043*"/>
    		<RowDefinition Height="0.741*"/>
    		<RowDefinition Height="0.216*"/>
    	</Grid.RowDefinitions>
