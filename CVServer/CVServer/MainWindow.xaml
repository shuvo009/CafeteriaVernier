			    			<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
			    				<GradientStop Color="#FFCECECE" Offset="0"/>
			    				<GradientStop Color="#FFE4E4E4" Offset="1"/>
			    				<GradientStop Color="#FF626262" Offset="0.504"/>
			    			</LinearGradientBrush>
			    		</Border.BorderBrush>
	    		<Image Source="{Binding Status, Converter={StaticResource CounterStatusToImagePath}}"/>
	    	</Border>        	
				</telerikTheme:RadFluidContentControl.Content>
