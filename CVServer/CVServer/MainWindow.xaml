								<dxe:TextEdit.Text>
									<Binding ElementName="EmployeeGridView" Path="SelectedItem.Name" Mode="TwoWay" UpdateSourceTrigger="LostFocus" ValidatesOnDataErrors="True">
										<Binding.ValidationRules>
											<local:EmployeeName/>
										</Binding.ValidationRules>
									</Binding>
								</dxe:TextEdit.Text>                    
							</dxe:TextEdit>
                            <dxe:TextEdit Margin="0,8,0,1" x:Name="EmployeePhone"  Grid.Row="5" TabIndex="3" MaxLength="19" NullText="Phone Number" Grid.Column="1" d:LayoutOverrides="Height">
